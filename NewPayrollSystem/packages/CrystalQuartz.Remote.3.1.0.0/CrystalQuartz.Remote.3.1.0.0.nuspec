<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>CrystalQuartz.Remote</id>
    <version>3.1.0.0</version>
    <title>CrystalQuartz panel for remote scheduler</title>
    <authors>Eugene Guryanov</authors>
    <owners>Eugene Guryanov</owners>
    <licenseUrl>http://dev.perl.org/licenses/</licenseUrl>
